{
    "name": "codemommy/configphp",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "http://www.codemommy.com",
    "description": "Read config files easily",
    "keywords": [
        "CodeMommy",
        "ConfigPHP",
        "Config",
        "PHP"
    ],
    "support": {
        "issues": "https://github.com/CodeMommy/ConfigPHP/issues",
        "source": "https://github.com/CodeMommy/ConfigPHP"
    },
    "authors": [
        {
            "name": "Candison November",
            "role": "Founder",
            "email": "kandisheng@163.com",
            "homepage": "http://www.kandisheng.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMommy\\ConfigPHP\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMommy\\ConfigPHP\\Script\\": "script/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "CodeMommy/TaskPHP": "*",
        "phpunit/phpunit": "*"
    },
    "scripts": {
        "clean": [
            "CodeMommy\\ConfigPHP\\Script\\CleanReport::start"
        ],
        "test": [
            "CodeMommy\\ConfigPHP\\Script\\CleanReport::start",
            "\"vendor/bin/phpunit\" -v",
            "start .report/index.html"
        ],
        "update-project": [
            "git pull",
            "composer self-update",
            "composer update"
        ]
    }
}